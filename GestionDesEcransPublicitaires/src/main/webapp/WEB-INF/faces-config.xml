<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">


	
	
<navigation-rule>
		
		<from-view-id>/pages/ajouterUtilisateur.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{utilisateurBean.ajouter}</from-action>
			<from-outcome>ajoute</from-outcome>
			<to-view-id>/pages/GestionUtilisateur.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		
		<from-view-id>/pages/ajouterClient.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{clientBean.ajouter}</from-action>
			<from-outcome>ajoute</from-outcome>
			<to-view-id>/pages/GestionClient.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		
		<from-view-id>/pages/ajouterMesaage.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{messageBean.ajouter}</from-action>
			<from-outcome>ajoute</from-outcome>
			<to-view-id>/pages/GestionMessage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
	
		<navigation-rule>
		
		<from-view-id>/pages/ajouterMagasin.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{magasinBean.ajouter}</from-action>
			<from-outcome>ajoute</from-outcome>
			<to-view-id>/pages/GestionMagasin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		
		<from-view-id>/pages/ajouterContrat.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{contratBean.ajouter}</from-action>
			<from-outcome>ajoute</from-outcome>
			<to-view-id>/pages/GestionContrat.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	

<navigation-rule>
		
		<from-view-id>/pages/modifierUtilisateur.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{utilisateurBean.modification}</from-action>
			<from-outcome>modifie</from-outcome>
			<to-view-id>/pages/listeUtilisateur.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
	<navigation-rule>
		
		<from-view-id>/pages/authentification.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{authentificationBean.connecter}</from-action>
			<from-outcome>accepte</from-outcome>
			<to-view-id>/pages/choixDesListes.xhtml</to-view-id>
		</navigation-case>
				<navigation-case>
			<from-action>#{authentificationBean.connecter}</from-action>
			<from-outcome>rejete</from-outcome>
			<to-view-id>/pages/authentification.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		
		<from-view-id>/pages/ajouterMagasin.xhtml</from-view-id>
		
		<navigation-case>
			<from-action>#{magasinBean.ajouter}</from-action>
			<from-outcome>ajoute</from-outcome>
			<to-view-id>/pages/GestionMagasin.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
 
 	<application>
		<resource-bundle>
			<base-name>messages</base-name>
			<var>msgs</var>
		</resource-bundle>

	</application>
 
 
 
</faces-config>